<Window x:Class="AYE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AYE"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Module 1" Click="Module1_Click" Margin="5"/>
            <Button Content="Module 2" Click="Module2_Click" Margin="5"/>
            <Button Content="Module 3" Click="Module3_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>